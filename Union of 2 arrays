/*https://practice.geeksforgeeks.org/problems/union-of-two-arrays3538/18*/
class Solution{
    public static int doUnion(int a[], int n, int b[], int m) 
    {
        //Your code here
        HashSet<Integer> hs=new HashSet<>();
        for(int i:a){
            hs.add(i);
        }
        for(int i:b){
            hs.add(i);
        }
        return hs.size();
        
    }
}
